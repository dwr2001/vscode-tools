{
  "name": "vscode-tools-package",
  "displayName": "vscode-tools-package",
  "description": "",
  "version": "0.0.0",
  "engines": {
    "node": ">=18"
  },
  "workspaces": ["packages/*"],
  "scripts": {
    "build": "npm run build --workspace=packages/protocol && npm run build --workspace=packages/webview-ui && npm run build --workspace=packages/extension",
    "check": "biome check --write",
    "dev": "npm run dev --workspaces",
    "dev:web": "npm run dev --workspace=packages/webview-ui",
    "format": "biome format --write",
    "package": "npm run build && npm run package --workspaces --if-present"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^1.0.0",
    "ai": "^5.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@rsbuild/core": "^1.4.0",
    "@rslib/core": "^0.12.0",
    "@types/node": "^18.x",
    "@types/vscode": "^1.82.0",
    "typescript": "^5.8.0"
  }
}
